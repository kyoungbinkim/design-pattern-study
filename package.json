    "singleton" : "npm start singleton",
    "observer" : "npm run node ./BehavioralPatterns/observer/index.js",
    "strategy" : "npm run node ./BehavioralPatterns/strategy/index.js",
    "adapter"  : "npm run node ./StructuralPatterns/adapter/index.js",
    "builder"  : "npm run node ./CreationalPatterns/builder/index.js",
    "prototype": "npm run node ./CreationalPatterns/prototype/index.js"
